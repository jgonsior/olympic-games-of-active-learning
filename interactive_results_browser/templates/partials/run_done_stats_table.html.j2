<table>
    <tr>
        {% for col in column_names %}
        {% if col == link_column %}
        <th>{{col}}</th>
        {% else %}
        <th>
            <a type="submit" href={{ url_for("show_results", experiment_name=experiment_name,
                **update_get_params(EXP_STRATEGY=col.value)) }}>
                {{ col.name }}
            </a>
        </th>
        {% endif %}
        {% endfor %}
    </tr>
    {% for row in row_data %}
    <tr>
        {% for col, row_ in zip(column_names, row) %}
        {% if col == link_column %}
        <td>
            <a type="submit" href={{ url_for("show_results", experiment_name=experiment_name,
                **update_get_params(EXP_DATASET=row_.value)) }}>
                {{ row_.name }}
            </a>
        </td>
        {% else %}
        <td>({{row_[0]}}, {{row_[1]}})</td>
        {% endif %}
        {% endfor %}
    </tr>
    {% endfor %}

</table>