{% extends "base.html.j2" %}
{% block title %}{{ experiment_name }}{% endblock %}
{% block body %}
<h1>{{ experiment_name }}</h1>



{% for viz_name,param_dict in full_exp_grid.items() %}
<h3>{{ viz_name }}</h3>
<ul>
    {% for k,v in param_dict.items() %}
    {% for v2 in v %}
    <li>
        <a
            href="{{ url_for('single_viz', experiment_name=experiment_name, viz_name=viz_name, viz_parameter=k, viz_param_value=v2) }} ">{{v2}}</a>
    </li>
    {% endfor%}
    {% endfor %}
</ul>
{% endfor %}
{% endblock %}